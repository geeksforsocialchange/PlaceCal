{
	"name": "placecal",
	"version": "2.0.0",
	"private": true,
	"dependencies": {
		"@hotwired/stimulus": "3.0.1",
		"@hotwired/turbo-rails": "7.1.1",
		"@nathanvda/cocoon": "1.2.14",
		"@rails/ujs": "6.1.7",
		"ansi-regex": "4.1.1",
		"async": "2.6.4",
		"axios": "0.24.0",
		"bootstrap": "4.6.2",
		"cocoon": "0.1.1",
		"datatables.net-bs4": "1.11.5",
		"esbuild": "0.14.38",
		"esbuild-rails": "1.0.3",
		"jquery": "3.6.1",
		"leaflet": "1.8.0",
		"lodash": "4.17.21",
		"minimist": "1.2.7",
		"popper.js": "1.16.1",
		"sass": "1.51.0",
		"select2": "4.0.13",
		"yarn": "1.22.19"
	},
	"devDependencies": {
		"husky": "8.0.2",
		"lint-staged": "13.0.3",
		"prettier": "2.7.1"
	},
	"peerDependencies": {
		"postcss": "8.4.19"
	},
	"scripts": {
		"build": "node esbuild.config.js",
		"build:css": "sass ./app/assets/stylesheets/application.sass ./app/assets/builds/application.css --no-source-map --load-path=node_modules",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"prepare": "husky install"
	},
	"lint-staged": {
		"*.{css,html,js,json,md,scss,yml}": "prettier --write",
		"{*.{rb,jbuilder,rake,ru},.simplecov,Gemfile,Rakefile}": "bundle exec rubocop --autocorrect"
	}
}
